@techreport{feautrier92aTR,
author=		"Feautrier, P.",
title=		"Some efficient solutions to the affine scheduling problem,
		{Part I}, One-dimensional Time",
institution=	{Labaratoire MASI, Institut Blaise Pascal},
number=		28,
month=		Apr,
year=		1992,
annote=		"This paper deals with the problem of finding closed form
	schedules as affine or piecewise affine functions of the iteration
	vector of a nested loop program.  The scheduling problem is reduced to
	a small sized parametric integer programming problem."
}

@techreport{feautrier92bTR,
author=		"Feautrier, P.",
title=		"Some efficient solutions to the affine scheduling problem,
		{Part II}, Multidimensional Time",
institution=	{Labaratoire MASI, Institut Blaise Pascal},
number=		78,
month=		Oct,
year=		1992,
annote=		"This paper extends the algorithms given in
	\cite{feautrier92a} to cases in which there is no affine schedule,
	i.e. to problems whose parallel complexity is polynomial, but not
	linear.  Linear schedules are generalized to multidimensional
	schedules with lexicographic ordering as a temporal succession.
	Multidimensional affine schedules are equivalent to polynomial
	schedules.  A heuristic is given to produce a multidimensional
	schedule of the smallest dimension."
}

